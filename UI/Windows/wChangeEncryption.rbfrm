o			n.ShowChooseFileDialog(nil)						r=n.Result			If r = NIL Then Return False			ug = r.File			Preferences.UserGuidePath = ug.ShellPath			End If						ug.Launch		End Function#tag EndMenuHandler#tag EndWindowCode#tag Events rbBlowfish	#tag Event		Sub Open()		  If CurrentEncryptionType = "Blowfish" Then		    Me.Value = True		  End If		  		  		End Sub	#tag EndEvent	#tag Event		Sub Action()		  CurrentEncryptionType = "Blowfish"		End Sub	#tag EndEvent#tag EndEvents#tag Events rbSerpent	#tag Event		Sub Open()		  If CurrentEncryptionType = "Serpent" Then		    Me.Value  = True		  End If		End Sub	#tag EndEvent	#tag Event		Sub Action()		  CurrentEncryptionType = "Serpent"		End Sub	#tag EndEvent#tag EndEvents#tag Events bOK	#tag Event		Sub Action()		  Close		End Sub	#tag EndEvent#tag EndEvents#tag Events rbAES	#tag Event		Sub Open()		  If CurrentEncryptionType = "AES" Then		    Me.Value  = True		  End If		End Sub	#tag EndEvent	#tag Event		Sub Action()		  CurrentEncryptionType = "AES"		End Sub	#tag EndEvent#tag EndEvents