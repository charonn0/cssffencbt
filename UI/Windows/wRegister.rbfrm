n		      wPrefs.bBuy.Visible = False		      wPrefs.bSn.Visible = False		      wPrefs.stTrial.Visible = False		      wPrefs.stUn.Visible = False		      wPrefs.stRegisteredTo.Visible = True		      wPrefs.stName.Text = regName		      wPrefs.stName.Visible = True		      wPrefs.StaticText2.Visible = True		    End If		    msgbox "Thanks for supporting us and purchasing BitClamp. Enjoy!"		    wMain.Show		    Close		  else		    'failed		    d.icon=MessageDialog.GraphicCaution		    d.ActionButton.Caption="Retry"		    d.CancelButton.Visible= True		    d.CancelButton.Caption="Report problem"		    d.Message="Incorrect license information."		    d.Explanation="The license information you entered was incorrect; make sure you entered both the name and code as they appeared in the e-mail sent to you. If the problem persists, e-mail us."		    Beep		    b=d.ShowModalWithin(self)		    Select Case b		    Case d.ActionButton		      //		    Case d.CancelButton		      showurl("mailto:bitclamp@fastforwardsw.com")		    End select		  end if		  		End Sub	#tag EndEvent#tag EndEvents