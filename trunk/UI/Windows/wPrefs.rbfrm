abled = False		  'bBrowse.Enabled = False		  'Else		  'stPath.Enabled = True		  'bBrowse.Enabled = True		  'End If		End Sub	#tag EndEvent#tag EndEvents#tag Events cbBounce	#tag Event		Sub Action()		  Preferences.BounceDockIcon = Me.Value		End Sub	#tag EndEvent	#tag Event		Sub Open()		  Me.Value = Preferences.BounceDockIcon		End Sub	#tag EndEvent#tag EndEvents#tag Events cbExtension	#tag Event		Sub Open()		  //Destroy list *evil cackle*		  Me.DeleteAllRows		  //Build list		  Me.Text = ("." + Preferences.DefaultFileExtension)		  Me.AddRow(Me.Text)		  For i as integer= 0 to UBound(Preferences.Extensions)		    //Check if duplicate, if not - add it		    If Preferences.Extensions(i) <> Me.Text AND Preferences.Extensions(i) <> "" Then		      //loop through and add dictionary of extensions.		      Me.AddRow(Preferences.Extensions(i))		    End If		  Next i		  Me.ListIndex = 0		End Sub	#tag EndEvent	#tag Event		Sub Close()		  //Write out the extensions to the preferences module.		  Dim duplicate as Boolean = False		  Dim i As Integer = 0		  While duplicate = False AND i <= UBound(Preferences.Extensions)		    //Check each cell of the array to see if this new value is already in the preferences array.		    If Me.Text = Preferences.Extensions(i) Then		      duplicate = True		    End If		    i = i + 1		  Wend		  //If it isnt a duplicate then add it as a row and onto the array.		  If duplicate = False Then		    Me.AddRow(Me.Text)		    Preferences.Extensions.Append(Me.Text)		  End If		End Sub	#tag EndEvent	#tag Event		Sub Change()		  Preferences.DefaultFileExtension = Mid(Me.Text, 2)		  		End Sub	#tag EndEvent	#tag Event		Sub TextChanged()		  Preferences.DefaultFileExtension = Mid(Me.Text, 2)		  		End Sub	#tag EndEvent#tag EndEvents#tag Events cbDecryptionConfirmation	#tag Event		Sub Action()		  Preferences.DecryptReminder = Me.Value		End Sub	#tag EndEvent	#tag Event		Sub Open()		  Me.Value = Preferences