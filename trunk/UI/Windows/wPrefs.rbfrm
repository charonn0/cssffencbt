lready in the preferences array.		    If Me.Text = Preferences.Extensions(i) Then		      duplicate = True		    End If		    i = i + 1		  Wend		  //If it isnt a duplicate then add it as a row and onto the array.		  If duplicate = False Then		    Me.AddRow(Me.Text)		    Preferences.Extensions.Append(Me.Text)		  End If		End Sub	#tag EndEvent	#tag Event		Sub Change()		  Preferences.DefaultFileExtension = Mid(Me.Text, 2)		  		End Sub	#tag EndEvent	#tag Event		Sub TextChanged()		  Preferences.DefaultFileExtension = Mid(Me.Text, 2)		  		End Sub	#tag EndEvent#tag EndEvents#tag Events cbEnableMimic	#tag Event		Sub Open()		  Me.Value = Preferences.EnableMimics		  rbChoose.Enabled = Me.Value		  rbAlways.Enabled = Me.Value		  stMimicPath.Enabled = Me.Value		  bbBrowse.Enabled = Me.Value		End Sub	#tag EndEvent	#tag Event		Sub Action()		  Preferences.EnableMimics = Me.Value		  rbChoose.Enabled =  Me.Value		  rbAlways.Enabled =  Me.Value		  If rbAlways.Value Then		    stMimicPath.Enabled =  Me.Value		    bbBrowse.Enabled =  Me.Value		  End If		  		End Sub	#tag EndEvent#tag EndEvents#tag Events rbChoose	#tag Event		Sub Open()		  Me.Value = Preferences.AskMimic		End Sub	#tag EndEvent	#tag Event		Sub Action()		  Preferences.AskMimic = Me.Value		  If Me.Value Then		    stMimicPath.Enabled = False		    bbBrowse.Enabled = False		  End If		End Sub	#tag EndEvent#tag EndEvents#tag Events rbAlways	#tag Event		Sub Action()		  Preferences.AskMimic = NOT Me.Value		  If Me.Value Then		    stMimicPath.Enabled = True		    bbBrowse.Enabled = True		  End If		End Sub	#tag EndEvent	#tag Event		Sub Open()		  ME.Value = NOT Preferences.AskMimic		End Sub	#tag EndEvent#tag EndEvents#tag Events bbBrowse	#tag Event		Sub Action()		  Dim temp as FolderItem		  temp = GetOpenFolderItem("")		  If temp =  Nil Then Return		  Preferences.MimicFile = temp		  If Len(Preferences.MimicFile.ShellPath.ReplaceAll("\", "") ) > 30 Then	